<?php

/**
 * @file
 * Update hooks for ICMS Product bundle.
 */

use Drupal\Core\StringTranslation\TranslatableMarkup;

/**
 * Implements hook_install().
 *
 * Sets the module weight to 50 to ensure deployment hooks run in the
 * correct order after core modules.
 */
function icms_bundle_product_logic_install() {
  module_set_weight('icms_bundle_product_logic', 50);
}
